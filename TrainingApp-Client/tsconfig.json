{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "jsx": "react-native",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "./src/**/*"
  ],
  "extends": "expo/tsconfig.base"
}
